echo 'clean classes/' && rm -fR classes/* &&
  echo 'clean generatedSources/' && rm -fR generatedSources/* &&
  find com/github/mapresultset/ -name "*.java" > mapsources.txt &&
  echo 'compile annotation processor sources:' && cat mapsources.txt &&
  javac -verbose -d classes/ @mapsources.txt &&
  find org/ -name "*.java" > testsources.txt &&
  echo 'compile testing sources with processor:' &&
  echo '---------------------' && cat testsources.txt && echo '---------------------' &&
  javac -verbose -cp classes/ -processor com.github.mapresultset.MappingProcessor -s generatedSources \
    -d classes/ @testsources.txt tests/TestProcessor.java &&
  echo 'Running TestProcessor' &&
  java -cp classes/:lib/mysql-connector-java-8.0.29.jar -ea tests.TestProcessor

